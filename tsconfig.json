/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@env/*": ["./src/environments/*"],
      "@assets/*": ["./src/assets/*"],
      "@app/*": ["./src/app/*"]
    }
  },
  "files": [
    "./src/main.ts",
    "./src/polyfills.ts",
  ],
  "include": [
    "./src/**/*.ts",
    "./src/**/*.d.ts",
    "./src/environments/*"
  ]
}
